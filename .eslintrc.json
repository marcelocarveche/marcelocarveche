{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier",
        "react-hooks"
    ],
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "no-redeclare": "off",
        "indent": [
            "error",
            4
        ]
    }
}